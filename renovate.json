{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "commitMessageExtra": "from {{{displayFrom}}} to {{{displayTo}}}",
  "kubernetes": {
    "fileMatch": ["\\.yaml$"]
  },
  "gomod": {
    "enabled": false
  },
  "terraform":{
    "enabled": false
  },
  "ansible-galaxy": {
    "enabled": false
  }
}
